{
  "name": "frontera-cms",
  "// version": "undefined",
  "// license": "undefined",
  "description": "The master CMS codebase for all downstream TACC stand alone CMS sites.",
  "scripts": {
    "build": "npm run build:css",
    "build:css": "postcss \"src/styles/exports/**/*.css\" --base src/styles/exports --dir taccsite_cms/static/site_cms/css"
  },
  "// scripts": {
    "build:css": "Compile files locally, to be committed to repo (quote glob to prevent OS parse)",
    "build:css // SEE": "https://github.com/postcss/postcss-cli/issues/142#issuecomment-310681302"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.tacc.utexas.edu:wma-cms/texascale-org.git"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {
    "postcss-import": "^12.0.1"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {
        "path": "./"
      },
      "cssnano": {
        "preset": "default"
      }
    }
  }
}
